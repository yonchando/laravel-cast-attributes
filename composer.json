{
    "name": "yonchando/laravel-cast-attirbute",
    "description": "Laravel model cast json columns to class object",
    "license": "MIT",
    "authors": [
        {
            "name": "Yon Chando",
            "email": "von.chando@gmail.com",
            "homepage": "https://github.com/yonchando"
        }
    ],
    "homepage": "https://github.com/yonchando/laravel-cast-mapping",
    "keywords": [
        "laravel",
        "laravel cast attribute",
        "laravel casting",
        "cast object",
        "cast json",
        "laravel json column"
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^7.0|8.0|9.0|10|11",
        "illuminate/container": "^8.12|^9.0|^10.0|^11.0",
        "illuminate/contracts": "^8.12|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0",
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7",
        "laravel/pint": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Yonchando\\CastAttributes\\": "CastAttributes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yonchando\\CastAttributes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CastMappingServiceProvider"
            ],
            "aliases": {
                "CastAttributes": "CastAttributes"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint ."
    }
}
